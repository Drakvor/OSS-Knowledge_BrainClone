# 이 파일을 복사해서 secrets.yaml로 만들고 실제 값을 입력하세요
# secrets.yaml은 .gitignore에 포함되어 있습니다

AZURE_SEARCH_API_KEY: "your-azure-search-api-key-here"
POSTGRES_PASSWORD: "your-postgres-password-here"
AZURE_OPENAI_API_KEY: "your-azure-openai-api-key-here"
AZURE_OPENAI_ENDPOINT: "https://your-openai.openai.azure.com/"

# Jenkins에서 사용할 크레덴셜 ID들
JENKINS_AZURE_SP: "azure-service-principal"
JENKINS_GITLAB_TOKEN: "gitlab-token"
JENKINS_ACR_CREDENTIALS: "acr-credentials"
